@*@model EmpManagment.Bol.ViewModels.UserAreaViewModels.ViewModels.ComplainantViewModel*@
@using System.Configuration;
@{
    ViewBag.Title = "Complaient's";
    var projectName = ConfigurationManager.AppSettings["SolutionProjectName"];
}
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Complaient's</h2>
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                @Html.ActionLink("Home", "Index", "Dashboard", new { area = "User" })
            </li>
            <li class="breadcrumb-item">
                <a><strong>Complaient's</strong></a>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">

    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox ">
                <a class="btn btn-primary" href="@Url.Action("createcomplaient","Complaient",new { Area="User"})"><i class="fa fa-plus"></i> Add New Complaient Server Side</a>
                @*<a class="btn btn-primary" href="@Url.Action("createcomplaient","Complaient",new { Area="User"})"><i class="fa fa-plus"></i> Add New Complaient With Ajax (Not Implimented)</a>*@
                <button type="submit" id="btnDeleteMultipaleComplaient" class="btn btn-danger"><i class="fa fa-remove"></i> Delete Multipale Complaient's</button>
                <div class="ibox-title">
                    <h5>Complaient's</h5>
                </div>
                <div class="ibox-content">
                    <input type="hidden" id="soluationName" value="@projectName" />
                    @*<input id="messageId" type="hidden" value="@Model.Message" />*@
                    <table id="tblComplaintList" class="table table-striped table-bordered table-hover dataTables-example">
                        <thead>
                            <tr>
                                <th>select</th>
                                <th>complainantName</th>
                                <th>complainantEmail</th>
                                <th>compaientDate</th>
                                <th>complaientCategoriesDescription</th>
                                <th>countryName</th>
                                <th>stateName</th>
                                <th>cityName</th>
                                <th>complaientDescription</th>
                                <th>complaientDate</th>
                                <th></th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox tablecontainerr">
                <a class="popup btn btn-primary" href="@Url.Action("CreateBikeCategories","Complaient",new {area="User"})"><i class="fa fa-plus"></i> Add New Categories</a>
                <button type="submit" id="btnDeleteMultipaleCategories" class="btn btn-danger"><i class="fa fa-remove"></i> Delete Multipale Categories</button>
                <div class="ibox-title">
                    <h5>Categorie's</h5>
                </div>
                <div class="ibox-content">
                    <table id="tblCategores" class="table table-striped table-bordered table-hover dataTables-example">
                        <thead>
                            <tr>
                                <th>select</th>
                                <th>name</th>
                                <th>description</th>
                                <th>createdDate</th>
                                <th>status</th>
                                <th></th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="myModal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title">Add New Bike Category</h3>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" id="myModalBodyDiv">

            </div>
            <div class="modal-footer">
                <a href="#" class="btn btn-default" data-dismiss="modal">Cancel</a>
                @*<a href="#" class="btn btn-default" ></a>*@
            </div>
        </div>
    </div>
</div>
@section Styles {
    <link href="~/Scripts/lib/dataTables/datatables.min.css" rel="stylesheet" />
}

@section Scripts {
    <script src="~/Scripts/lib/dataTables/datatables.min.js"></script>
    <script src="~/Scripts/lib/dataTables/dataTables.bootstrap4.min.js"></script>
    <script src="~/Scripts/js/ComplaientList.js"></script>
}